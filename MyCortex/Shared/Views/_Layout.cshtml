        $(document).ready(function () {
            const favicon = document.getElementById("ProductImg");
            favicon.setAttribute("href", ProductName[1]);

            var ProductNameId = document.getElementById('ProductName');
            console.log(ProductName[0]);
            if ('textContent' in ProductNameId) {
                ProductNameId.textContent = ProductName[0];
            } else {
                ProductNameId.innerText = ProductName[0];
            }

            var productnamespan = document.getElementById('productnamespan');

            if ('textContent' in productnamespan) {
                productnamespan.textContent = ProductName[2];
            } else {
                productnamespan.innerText = ProductName[2];
            }
        });

    var nameList = jQuery.parseJSON('@Html.Action("LoginDetails", "Home")');
    $(document).ready(function() {
        var usernamespan = document.getElementById('usernamespan');

        if ('textContent' in usernamespan) {
            usernamespan.textContent = nameList[8];
        } else {
            usernamespan.innerText = nameList[8];
        }

        var Show_UserType = document.getElementById('Show_UserType');

        if ('textContent' in Show_UserType) {
            Show_UserType.textContent = nameList[10];
        } else {
            Show_UserType.innerText = nameList[10];
        }
        document.getElementById("profileIcon").src = nameList[1];
        document.getElementById("profileIcon").title = "Last Login: " + nameList[0];
        if (nameList[3] == "2")
        {
            document.getElementById("divPatientType").style.display = "none";
        }
        else
        {
            document.getElementById("divPatientType").style.display = "none";
        }

        var CountList = jQuery.parseJSON('@Html.Action("GetUnread_Notification_Count", "Home")');
        $(document).ready(function () {
            document.getElementById("UnreadCountIcon").title = "Unread Notifications: " + CountList[0];
            var NotificationCount = document.getElementById('notificationCount');
            NotificationCount.textContent = CountList[0];
            if (CountList[1] == 1 || CountList[1] == 3) {
                document.getElementById("mydiv").style.display = "none";
            }
        });
    });
