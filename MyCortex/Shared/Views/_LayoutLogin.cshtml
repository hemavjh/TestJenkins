    <script type="text/javascript">
        var BuildList = jQuery.parseJSON('@Html.Action("BuildDetails", "Home")');
        $(document).ready(function () {
            var Buildnamespan = document.getElementById('Buildnamespan');

            if ('textContent' in Buildnamespan) {
                Buildnamespan.textContent = BuildList[0];
            } else {
                Buildnamespan.innerText = BuildList[0];
            }
            if (BuildList[1] == 'https://hive.clinic/') {
                Buildnamespan.innerText = BuildList[0];
            } else if (BuildList[1] == 'https://uat.hive.clinic/') {
                Buildnamespan.innerText = 'UAT- ' + BuildList[0];
            } else if (BuildList[1] == 'https://qa.myhealthae.com/') {
                Buildnamespan.innerText = BuildList[0];
            } else if (BuildList[1] == 'https://telemedicine.myhealthae.com/') {
                Buildnamespan.innerText = 'QA- ' + BuildList[0];
            } else if (BuildList[1] == 'https://uat.mycortex.health/') {
                Buildnamespan.innerText = BuildList[0];
            }
        });
        var PhotoList = jQuery.parseJSON('@Html.Action("LoginPageLogoDetails", "Home")');
        $(document).ready(function () {
            document.getElementById.toString("InstLogo").src = PhotoList[0];
        });

        var ProductName = jQuery.parseJSON('@Html.Action("GetProduct_Details", "Home")');
        $(document).ready(function () {
            const favicon = document.getElementById("ProductImg");
            favicon.setAttribute("href", ProductName[3]);

            var ProductNameId = document.getElementById('ProductName');
            //console.log(ProductNameId);
            if ('textContent' in ProductNameId) {
                ProductNameId.textContent = ProductName[0];
                show_login(ProductName[0]);
            } else {
                ProductNameId.innerText = ProductName[0];
                show_login(ProductName[0]);
            }

            //var productnamespan = document.getElementById('productnamespan');
            //if ('textContent' in productnamespan) {
            //    productnamespan.textContent = ProductName[0];
            //} else {
            //    productnamespan.innerText = ProductName[0];
            //}
        });
        function show_login(val) {
            if (val == 'MyCortex') {
                //document.getElementById("loginHel").style.display = "none";
                //document.getElementById("loginCor").style.display = "block";
                document.getElementById("loginhead").style.display = "block";
            } else {
                //document.getElementById("loginHel").style.display = "block";
                //document.getElementById("loginCor").style.display = "none";
                document.getElementById("loginhead").style.display = "none";
            }
        }
    </script>
